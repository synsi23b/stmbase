  "version": "0.2.0",
  "configurations": [
    {
      "name": "Generate Synrpc Messages STM32",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/stmbase/synrpc_generator/synrpcgen.py",
      "console": "integratedTerminal",
      "args": ["STM32"],
    },
    {
        "name": "Run synrpc tests",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/test_usbcon.py",
        "console": "integratedTerminal"
    },
    {
        "name": "Generate stm32f103c8",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/stmbase/startproject.py",
        "console": "integratedTerminal",
        "args": ["STM32", "stm32f103c8", "8192"]
    },
    {
        "name": "Generate stm32f401ccu6",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/stmbase/startproject.py",
        "console": "integratedTerminal",
        "args": ["STM32", "stm32f401ccu6", "20480"]
    }
  ]
}